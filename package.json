{
  "name": "pills-tracker-web-app",
  "productName": "Pills tracker web app",
  "description": "Repository for pills tracker web app.",
  "version": "1.0.0",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "main": "app/background.js",
  "keywords": [
    "app",
    "pills",
    "health",
    "tracker",
    "web app"
  ],
  "author": {
    "name": "Alex Devero",
    "url": "https://github.com/alexdevero"
